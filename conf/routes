# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.GithubController.index

#Github
+ nocsrf
POST     /search                    controllers.GithubController.search(request: Request)


GET      /getRepositoryDetails/:userName/:repositoryName      controllers.GithubController.getRepositoryDetails(userName : String, repositoryName : String)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
